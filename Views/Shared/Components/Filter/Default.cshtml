@{
}

@model IEnumerable<MasterBurial2>

@foreach (var loc in Model)
{
    <a asp-action="Index" asp-controller="Home"
       asp-route-teamId="@loc.BurialId"
       asp-route-teamName="@loc.LocConcat"
       asp-route-pagenum="1"
       class="btn btn-block @(loc.LocConcat == ViewBag.SelectedTeam ? "btn-primary" : "btn-outline-secondary")">@loc.LocConcat</a>
    <br />
}
