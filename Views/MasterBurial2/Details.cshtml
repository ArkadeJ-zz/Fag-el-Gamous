@model Fag_el_Gamous.Models.MasterBurial2

@{
    ViewData["Title"] = "Burial Record Details";
    ViewBag.pageType = "profile-page";
    ViewBag.Background = "/img/tent.jpg";
}

<h1>Burial Record #@Model.BurialId</h1>



<!-- Section for the Photo Gallery -->
<section class="gallery-block grid-gallery">
    <div class="container">
        <div class="heading">
            <h2>Photos</h2>
        </div>
        <div class="row">
            @foreach (var pic in ViewBag.PhotoBurialList)
            {


                @if (Model.BurialId == pic.BurialId)
                {

                    <div class="col-md-6 col-lg-4 item">
                        <a class="lightbox" href="@pic.UrlLink">
                            <img class="img-fluid image scale-on-hover" style="padding: 20px" src="@pic.UrlLink" alt="Picture for Burial ID: @pic.BurialId">
                        </a>
                    </div>

                }
                else if(pic.PhotoId == 1)
                {
                    
                    
                    <div class="col-md-6 col-lg-4 item">
                        <a class="lightbox">
                            <img class="img-fluid image scale-on-hover" style="padding: 20px" src="~/img/noImage.jpg" alt="Picture for Burial ID: @pic.BurialId">
                        </a>
                    </div>
                }
            }
        </div>
    </div>
</section>
<script src="https://cdnjs.cloudflare.com/ajax/libs/baguettebox.js/1.10.0/baguetteBox.min.js"></script>
<script>
            baguetteBox.run('.grid-gallery', { animation: 'slideIn'});
</script>




